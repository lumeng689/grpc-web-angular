{
  "extends": "../tslint.json",
  "linterOptions": {
    "exclude": [
      "bin",
      "src/app/pb/*",
      "app/pb"
    ]
  },
  "rules": {
    "class-name": false,
    "directive-selector": [
      true,
      "attribute",
      "app"
//      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
